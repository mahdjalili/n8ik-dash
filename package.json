{
    "name": "n8ik-dash",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "dev:docker": "docker compose -f docker-compose.dev.yml up --build --remove-orphans",
        "update:clients": "bun clients/update.mjs",
        "lint": "turbo lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "@workspace/eslint-config": "workspace:*",
        "@workspace/typescript-config": "workspace:*",
        "prettier": "^3.7.4",
        "turbo": "^2.6.3",
        "chokidar": "^3.6.0",
        "typescript": "5.7.3"
    },
    "packageManager": "bun@1.3.12",
    "workspaces": {
        "packages": [
            "apps/*",
            "packages/*",
            "clients/*"
        ],
        "catalogs": {
            "state-manager": {
                "zustand": "^5.0.6",
                "nuqs": "^2.4.3"
            },
            "form": {
                "formik": "^2.4.6",
                "yup": "^1.6.1",
                "zod": "^3.24.2"
            },
            "api": {
                "@tanstack/react-query": "^5.80.6",
                "@tanstack/react-query-devtools": "^5.80.6",
                "axios": "^1.8.4"
            },
            "next": {
                "react": "^19.2.3",
                "react-dom": "^19.2.3",
                "next": "^16.0.10",
                "next-themes": "^0.4.6"
            },
            "tailwind": {
                "tailwindcss": "^4.1.17",
                "@tailwindcss/postcss": "^4.1.17"
            },
            "material": {
                "@mui/material": "^7.0.1",
                "@mui/material-nextjs": "^7.0.0",
                "@mui/icons-material": "^7.0.1",
                "@mui/x-data-grid": "^7.28.2",
                "date-fns-jalali": "^4.1.0-0",
                "@mui/x-date-pickers": "^8.9.0",
                "@mui/x-tree-view": "^7.28.1",
                "@mui/lab": "^7.0.0-beta.10",
                "@emotion/cache": "^11.14.0",
                "@emotion/react": "^11.14.0",
                "@emotion/styled": "^11.14.0"
            },
            "chart": {
                "apexcharts": "^4.7.0",
                "react-apexcharts": "^1.7.0"
            },
            "build": {
                "@swc/cli": "^0.7.7",
                "@swc/core": "^1.11.24"
            },
            "persian-tools": {
                "@persian-tools/persian-tools": "^3.6.0",
                "jalaliday": "^3.1.0",
                "@zoomit/dayjs-jalali-plugin": "^0.1.11",
                "moment-jalaali": "^0.10.4"
            },
            "utils": {
                "lodash": "^4.17.21",
                "es-toolkit": "^1.34.1"
            }
        }
    }
}
